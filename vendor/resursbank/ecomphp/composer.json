{
    "name": "resursbank/ecomphp",
    "description": "Resurs Bank AB ecommerce library for PHP.",
    "license": "Apache-2.0",
    "require": {
        "tornevall/tornelib-php-bitmask": "^6.1",
        "tornevall/tornelib-php-netcurl-deprecate-60": "^6.0",
        "tornevall/tornelib-php-network": "^6.1"
    },
    "config": {
        "preferred-install": {
            "tornevall/tornelib-php-netcurl-deprecate-60": "dist",
            "*": "source"
        }
    },
    "authors": [
        {
            "name": "Tomas Tornevall",
            "email": "tomas.tornevall@resurs.se"
        }
    ],
    "autoload": {
        "files": [
            "source/classes/ecomhooks.php",
            "source/classes/rbapiloader.php"
        ],
        "psr-4": {
            "Resursbank\\RBEcomPHP\\": "source/classes/rbapiloader.php"
        }
    }
}
